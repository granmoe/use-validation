{
  "name": "use-validation",
  "version": "0.2.2",
  "main": "dist/index.js",
  "license": "MIT",
  "author": "Matt Granmoe",
  "description": "",
  "keywords": [],
  "files": [
    "dist/"
  ],
  "scripts": {
    "test": "NODE_ENV=test && jest",
    "compile": "NODE_ENV=production babel index.js -d dist",
    "prepublishOnly": "npm run compile"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn test"
    }
  },
  "lint-staged": {
    "*.(js|json|md)": [
      "prettier --write",
      "xo --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "eslint-config-xo-react": "^0.17.0",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.1.3",
    "jest": "^23.6.0",
    "lint-staged": "^8.0.4",
    "prettier": "^1.14.3",
    "prop-types": "^15.6.2",
    "react": "16.7.0-alpha.0",
    "react-dom": "^16.7.0-alpha.0",
    "react-testing-library": "^5.2.3",
    "regenerator-runtime": "^0.12.1",
    "styled-components": "4.0.3",
    "xo": "^0.23.0"
  },
  "peerDependencies": {
    "react": ">=16.7.0-alpha.0"
  },
  "babel": {
    "env": {
      "production": {
        "presets": [
          "@babel/preset-env"
        ]
      },
      "test": {
        "presets": [
          "@babel/preset-env",
          "@babel/preset-react"
        ]
      }
    }
  },
  "prettier": {
    "semi": false,
    "bracketSpacing": true,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "xo": {
    "extends": "xo-react",
    "prettier": true,
    "space": true,
    "rules": {
      "no-void": "off",
      "react/require-default-props": "off",
      "no-unused-vars": [
        "error",
        {
          "varsIgnorePattern": "^_",
          "argsIgnorePattern": "^_"
        }
      ]
    },
    "global": [
      "test"
    ]
  },
  "dependencies": {}
}
